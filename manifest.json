{
  "name": "Text Parser",
  "description": "This is a text parsing tool that helps in parsing the text and extracting the required information.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
        "16": "icons/icon.png",
        "32": "icons/icon-32.png",
        "128": "icons/icon-128.png"
    }
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  }
}
